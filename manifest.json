{
	"manifest_version": 3,
	"version": "1.0",

	"name": "Bookmark Search",
	"description": "Uses the chrome.bookmarks API to recommend forgotten bookmarks from the user's repository.",

	"permissions": ["bookmarks", "activeTab"],

	"omnibox": { "keyword" : "find" },

	"background": {
		"service_worker": "background.js"
	},

	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon-16.png",
			"32": "icon-32.png",
			"48": "icon-48.png",
			"128": "icon-128.png"
		}
	},

	"icons": {
		"16": "icon-16.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	}
}